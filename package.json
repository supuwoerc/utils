{
  "name": "@supuwoerc/utils",
  "type": "module",
  "version": "1.0.0",
  "description": "a collection of utility functions for JavaScript/TypeScript",
  "author": {
    "name": "Idris",
    "email": "supuwoerc@gmail.com",
    "url": "https://github.com/supuwoerc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supuwoerc/utils"
  },
  "keywords": [
    "utils"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "doc": "typedoc --options typedoc.json",
    "release": "pnpm test && pnpm build && pnpm release-it",
    "lint": "eslint . --fix",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "throttle-debounce": "^5.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@release-it/conventional-changelog": "^10.0.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.18.12",
    "@types/throttle-debounce": "^5.0.2",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "camelcase": "^8.0.0",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.7.4",
    "release-it": "^19.0.6",
    "release-it-pnpm": "^4.6.6",
    "terser": "^5.44.0",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3",
    "vite": "^7.1.10",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.15"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
